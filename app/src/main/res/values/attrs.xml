<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color" />
        <attr name="innerColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="stepTextSize" format="dimension" />
        <attr name="stepTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ColorTrackTextView">
        <attr name="originColor" format="color" />
        <attr name="changeColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ProgressBarView">
        <attr name="progressOuterColor" format="color" />
        <attr name="progressInnerColor" format="color" />
        <attr name="progressBorderWidth" format="dimension" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RatingBarView">
        <attr name="ratingBarStarNormal" format="reference" />
        <attr name="ratingBarStarFocus" format="reference" />
        <attr name="ratingBarGradeNumber" format="integer" />
        <attr name="ratingBarPadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LetterSideBar">
        <attr name="letterSideBarTextSize" format="dimension" />
        <attr name="letterSideBarCurrentColor" format="color" />
        <attr name="letterSideBarTextColor" format="color" />
    </declare-styleable>
</resources>